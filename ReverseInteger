class Solution {
    public int reverse(int x) {
        long val =0;
        while(x !=0){
            int r = x % 10;
            x = x /10;
            val = val * 10 + r;
            if(val<Integer.MIN_VALUE || val>Integer.MAX_VALUE){
                return 0;
            }
            
        }
      return (int)(val);
    }
}
